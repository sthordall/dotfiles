[hub]
  protocol = https
[user]
  name = AUTHORNAME
  email = AUTHOREMAIL
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  pl = pull
  rb = rebase
  cl = clone
  ciw = commit
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[core]
  excludesfile = ~/.gitignore
  editor = vim
[apply]
  whitespace = nowarn
[mergetool]
  keepBackup = false
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[help]
  autocorrect = 1
[push]
  default = simple
[github]
  user = GITHUB_USER
[url "https://github.com/"]
  insteadOf = git://github.com
[credential]
  helper = store

