[hub]
        protocol = https
[user]
        name = AUTHORNAME
        email = AUTHOREMAIL
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[difftool]
        prompt = false
[help]
        autocorrect = 1
[push]
        default = simple
[github]
        user = GITHUB_USER
[url "https://github.com/"]
        insteadOf = git://github.com

